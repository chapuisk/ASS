Series = {{Proceedings of SPIE}},
Year = {{2005}},
Volume = {{5818}},
Pages = {{181-192}},
Note = {{Conference on Independent Component Analyses, Wavelets, Unsupervised
   Smart Sensors, and Neural Networks III, Orlando, FL, MAR 30-APR 01, 2005}},
Organization = {{SPIE; Ball Aerosp \& Technol Corp; Univ Cent Florida, Coll Opt \&
   Photon; Florida Space Inst; IEEE Computat Intelligence Soc; Int Neural
   Network Soc; Int Soc Professional Thermographers; Michigan Aerosp Corp;
   Snell Infrared; Swedish Defense Res Agcy; Univ Cent Florida}},
Abstract = {{A principled theoretical framework for a novel probabilistic ``source
   separation{''} model for lattice data is derived using an invariant
   extension of the Extreme Physical Information (EPI) theory. The pdfs of
   the estimated sources are parameterized by a unique ansatz that
   satisfies the invariant EPI (IEPI) game corollary. Application of the
   ansatz yields a dimensionless form of the Fisher information matrix
   (FIM). The criterion for ``source separation{''} is the determination of
   operators that diagonalize the Fisher information matrix (FIM). The
   discrete IEPI ``source separation{''} model is ascribed a quantum
   mechanical connotation. Numerical examples demonstrate the efficacy of
   the IEPI model in density estimation.}},
DOI = {{10.1117/12.603382}},
ISSN = {{0277-786X}},
EISSN = {{1996-756X}},
ISBN = {{0-8194-5803-1}},
Unique-ID = {{ISI:000229698000018}},
}

