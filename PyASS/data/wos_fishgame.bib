@article{ ISI:000311127300005,
Author = {Tremon, Anne-Christine},
Title = {{Social rationality and scales of action: inter-ethnic relations in
   cockfighting and game-fishing, Raiatea, French Polynesia}},
Journal = {{ETHNIC AND RACIAL STUDIES}},
Year = {{2012}},
Volume = {{35}},
Number = {{12}},
Pages = {{2116-2133}},
Abstract = {{This article challenges the notion of economic rationality as a
   criterion for explaining ethnic boundary maintenance. It offers an
   ethnographic analysis of inter-ethnic relations in the context of games
   (cockfights and game-fishing contests) in the island of Raiatea (French
   Polynesia). Although all players engage in the same basic gambling
   practices, money is differentially scaled and mobilized by the Tahitian
   and Chinese participants. Building on recent pragmatic approaches to
   rationality, it is shown that the players' rationalities differ not from
   the point of view of economic maximization, but only in so far as they
   participate in social relations at different scales.}},
DOI = {{10.1080/01419870.2011.632018}},
