@article{ ISI:000208042600005,
Author = {Bressan, Alberto and Shen, Wen},
Title = {{MEASURE-VALUED SOLUTIONS FOR A DIFFERENTIAL GAME RELATED TO FISH
   HARVESTING}},
Journal = {{SIAM JOURNAL ON CONTROL AND OPTIMIZATION}},
Year = {{2009}},
Volume = {{47}},
Number = {{6}},
Pages = {{3118-3137}},
Abstract = {{In this paper we consider a model for the harvesting of marine
   resources, described by an elliptic equation. Since the cost functionals
   have sublinear growth with respect to the pointwise intensity of fishing
   effort, optimal solutions are in general measure-valued. For the control
   problem in one space dimension, we prove the existence of optimal
   strategies. Uniqueness is established within a class of measures with
   small total mass. We also study the differential game, modeling the
   presence of several competing fishing companies, and prove the existence
   of a Nash equilibrium solution. This is obtained as a fixed point of a
   continuous transformation in a space of positive Radon measures.}},
DOI = {{10.1137/07071007X}},
ISSN = {{0363-0129}},
